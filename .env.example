# Sign Protocol API Key
NEXT_PUBLIC_SIGN_PROTOCOL_API_KEY=your_sign_protocol_api_key_here

# Sepolia RPC URL (Infura)
NEXT_PUBLIC_SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_infura_project_id

# Private Key (for signing transactions)
NEXT_PUBLIC_PRIVATE_KEY=your_private_key_here

# RainbowKit Project ID
NEXT_PUBLIC_RAINBOWKIT_PROJECT_ID=your_rainbowkit_project_id

# Merchant Address
NEXT_PUBLIC_MERCHANT_ADDRESS=your_merchant_address_here

# Purchase Contract Address
NEXT_PUBLIC_PURCHASE_CONTRACT_ADDRESS=your_purchase_contract_address_here 